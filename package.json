{
  "name": "angular-webpack",
  "version": "1.0.0",
  "description": "Angular 1.6 Stack",
  "main": "src/index.js",
  "repository": "https://github.com/scream_xy/angular-webpack-typescript.git",
  "dependencies": {
    "angular": "~1.6.0",
    "angular-animate": "~1.6.0",
    "angular-aria": "~1.6.0",
    "angular-cookies": "~1.6.0",
    "angular-i18n": "~1.6.0",
    "angular-material": "^1.1.1",
    "angular-messages": "~1.6.0",
    "angular-moment": "^0.10.3",
    "angular-resource": "~1.6.0",
    "angular-sanitize": "~1.6.0",
    "angular-ui-router": "^0.3.2",
    "momentjs": "^2.0.0",
    "rxjs": "^5.0.3",
    "zone.js": "^0.7.5"
  },
  "devDependencies": {
    "@angular/core": "2.4.3",
    "@angular/compiler": "2.4.3",
    "@types/angular": "1.6.0",
    "@types/core-js": "0.9.35",
    "@types/jasmine": "2.5.40",
    "@types/protractor": "4.0.0",
    "@types/selenium-webdriver": "2.53.39",
    "@types/typescript": "2.0.0",
    "angular-mocks": "~1.6.0",
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-latest": "^6.16.0",
    "codelyzer": "^2.0.0-beta.4",
    "concurrently": "^3.1.0",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.8.1",
    "ng-annotate-loader": "^0.2.0",
    "node-sass": "^4.1.1",
    "phantomjs-prebuilt": "^2.1.7",
    "postcss-loader": "^1.2.1",
    "precss": "^1.4.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "ts-loader": "^1.3.3",
    "tslint": "^4.3.1",
    "typescript": "^2.1.5",
    "webpack": "^2.1.0-beta.28",
    "webpack-dev-server": "^2.1.0-beta.12"
  },
  "scripts": {
    "lint": "tslint \"src/**/*.ts\"",
    "test": "karma start && echo ok || echo not ok",
    "prestart": "npm install",
    "start": "npm run-script dev",
    "dev": "node node_modules/.bin/webpack-dev-server --env.dev --colors --hot --inline --content-base src --host 0.0.0.0 --port 9000",
    "build": "npm test && rm -rf dist/ && node node_modules/.bin/webpack --env.prod -p",
    "dist": "npm test && rm -rf dist/ && node node_modules/.bin/webpack --env.prod -p && node node_modules/.bin/webpack-dev-server --env.prod -p --compress --content-base dist --host 0.0.0.0 --port 9000",
    "prefullbuild": "npm install",
    "fullbuild": "npm run build",
    "protractor:update": "node node_modules/protractor/bin/webdriver-manager update",
    "clean": "npm run clean:src && npm run clean:e2e && npm run clean:stuff",
    "clean:src": "find src/ -type f -name '*.js' -delete && find src/ -type f -name '*.js.map' -delete && find src/ -empty -type d -delete",
    "clean:e2e": "find e2e/ -type f -name '*.js' -delete && find e2e/ -type f -name '*.js.map' -delete && find e2e/ -empty -type d -delete",
    "clean:stuff": "rm -rf coverage && rm -rf dist/* && rm -rf .tmp"
  },
  "author": "scream_xy",
  "license": "MIT"
}
